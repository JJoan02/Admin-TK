  "type": "module",
  "scripts": {
    "start": "node dist/start.js",
    "dev": "nodemon --exec ts-node-esm src/app.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write .",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "clean": "rimraf node_modules && rimraf sessions && rimraf backups && rimraf database.sqlite && rimraf dist",
    "backup": "node dist/utils/dbBackup.js",
    "prepare": "husky install",
    "audit": "npm audit --production",
    "healthcheck": "node dist/utils/healthCheck.js",
    "profile": "node --inspect-brk",
    "analyze": "source-map-explorer 'dist/*.js'"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "whatsapp-bot",
    "nodejs",
    "admin-tk"
  ],
  "author": "JJoan02",
